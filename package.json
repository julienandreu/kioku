{
    "author": {
        "email": "julienandreu@me.com",
        "name": "Julien Andreu",
        "url": "https://github.com/julienandreu/"
    },
    "ava": {
        "extensions": {
            "ts": "module"
        },
        "nodeArguments": [
            "--import=tsx"
        ],
        "timeout": "1m",
        "workerThreads": false
    },
    "bugs": {
        "url": "https://github.com/julienandreu/kioku/issues"
    },
    "dependencies": {
        "lru-cache": "^11.1.0",
        "mimic-function": "^5.0.1"
    },
    "description": "記憶 (ki-o-ku) = MEMORY in Japanese - Memoize functions and cache results",
    "devDependencies": {
        "@typescript/native-preview": "^7.0.0-dev.20250710.1",
        "ava": "^6.1.3",
        "delay": "^6.0.0",
        "tsd": "^0.32.0",
        "tsx": "^4.20.3",
        "xo": "^1.1.1"
    },
    "engines": {
        "node": ">=22"
    },
    "exports": {
        "default": "./dist/index.js",
        "types": "./dist/index.d.ts"
    },
    "files": [
        "dist"
    ],
    "funding": "https://github.com/julienandreu/kioku?sponsor=1",
    "homepage": "https://github.com/julienandreu/kioku#readme",
    "keywords": [
        "kioku",
        "memoize",
        "function",
        "mem",
        "memo",
        "memoization",
        "cache",
        "caching",
        "lru-cache",
        "optimize",
        "performance",
        "ttl",
        "expire",
        "promise",
        "generator"
    ],
    "license": "MIT",
    "name": "kioku",
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com/julienandreu/kioku.git"
    },
    "scripts": {
        "build": "tsgo",
        "prebuild": "rm -rf dist",
        "prepack": "npm run build",
        "lint": "xo",
        "lint:fix": "xo --fix",
        "test": "npm run lint && npm run build && tsd --typings dist/index.d.ts"
    },
    "sideEffects": false,
    "type": "module",
    "version": "1.0.0"
}
