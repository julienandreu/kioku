{
    "author": {
        "email": "julienandreu@me.com",
        "name": "Julien Andreu",
        "url": "https://github.com/julienandreu/"
    },
    "ava": {
        "extensions": {
            "ts": "module"
        },
        "nodeArguments": [
            "--import=tsx"
        ],
        "timeout": "1m",
        "workerThreads": false
    },
    "bugs": {
        "url": "https://github.com/julienandreu/kioku/issues"
    },
    "dependencies": {
        "lru-cache": "^11.1.0",
        "mimic-function": "^5.0.1"
    },
    "description": "記憶 (ki-o-ku) = MEMORY in Japanese - Memoize functions and cache results",
    "devDependencies": {
        "@types/node": "^24.0.14",
        "@typescript/native-preview": "^7.0.0-dev.20250715.1",
        "c8": "^10.1.3",
        "expect-type": "^1.2.2",
        "tsx": "^4.20.3",
        "xo": "^1.1.1"
    },
    "engines": {
        "node": ">=22"
    },
    "exports": {
        "default": "./dist/index.js",
        "types": "./dist/index.d.ts"
    },
    "files": [
        "dist"
    ],
    "funding": "https://github.com/julienandreu/kioku?sponsor=1",
    "homepage": "https://github.com/julienandreu/kioku#readme",
    "keywords": [
        "kioku",
        "memoize",
        "function",
        "mem",
        "memo",
        "memoization",
        "cache",
        "caching",
        "lru-cache",
        "optimize",
        "performance",
        "ttl",
        "expire",
        "promise",
        "generator"
    ],
    "license": "MIT",
    "name": "kioku",
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com/julienandreu/kioku.git"
    },
    "scripts": {
        "build": "tsgo",
        "lint": "xo",
        "lint:fix": "xo --fix",
        "prebuild": "rm -rf dist",
        "prepack": "npm run build",
        "test": "npm run test:code && npm run test:types",
        "test:code": "tsx src/test.ts",
        "test:types": "tsgo --project tsconfig.json --noEmit src/index.test-d.ts",
        "test:coverage": "npm run build && c8 --reporter=text --reporter=html --reporter=lcov node --test dist/test.js"
    },
    "sideEffects": false,
    "type": "module",
    "version": "1.0.0"
}
